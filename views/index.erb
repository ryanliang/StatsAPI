<p>Stats</p>

<h1>Listing Stats</h1>

<table>
  <tr>
    <th>Suite</th>
    <th>Reporting Date</th>
    <th>Year</th>
    <th>Release</th>
    <th>Area</th>
    <th>Size</th>
    <th>Raw_Stats</th>
    <th>Duration</th>
    <th>Total</th>
    <th>Failures</th>
    <th>Pass</th>
    <th>Updated At</th>
    <th>Comments</th>
    
  </tr>

<% @executions.each do |exec| %>
  <tr>
    <% area = Area[exec.area_id] %>
    <td><%= area.test_suite %></td>
    <td><%= exec.report_date %></td>
    <td><%= exec.year %></td>
    <td><%= exec.release %></td>
    <td><%= area.name %></td>
    <td><%= area.size %></td>
    <td><%= exec.raw_stats %></td>
    <td><%= exec.duration %> Seconds</td>
    <td><%= exec.total %></td>
    <td><%= exec.failed %></td>
    <td><%= exec.total - exec.failed %></td>
    <td><%= exec.updated_at.strftime("%m/%d/%Y %H:%M") %></td>
    <td><%= exec.comment %></td>
    
  </tr>
<% end %>
</table>

<br />
